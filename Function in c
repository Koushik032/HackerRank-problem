#include<stdio.h>
void max_number(int a,int b,int c,int d)
{
    if(a>b && a>c && a>d)
    {
        printf("%d",a);
    }
    else if(b>a &&& b>c && b>d)
    {
        printf("%d",b);
    }
    else if(c>a &&& c>b && c>d)
    {
        printf("%d",c);
    }
    else if(d>a &&& d>c && d>b)
    {
        printf("%d",d);
    }
}
int main()
{
    int p,q,r,s;

    scanf("%d %d %d %d",&p,&q,&r,&s);

max_number(p,q,r,s);
    return 0;
}
