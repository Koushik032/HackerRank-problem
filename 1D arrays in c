#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
int s[100000];
int n,i,sum=0;
scanf("%d",&n);
for(i=0;i<n;i++)
{
    scanf("%d",&s[i]);
}

for(i=0;i<n;i++)
{
    sum=sum+s[i];
}

printf("%d",sum);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
